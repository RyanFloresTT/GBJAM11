* {
  -unity-font: url("project://database/Assets/Art/Fonts/EarlyGameBoy.ttf");
  -unity-font-definition: initial;
}

#title-start {
  position: absolute;
  bottom: 0px;
  left: 0;
  color: #ffffff;
  font-size: 32px;
  width: 100%;
  align-items: center;
  justify-content: center;
  height: 16%;
  flex-direction: row;
  opacity: 0;
}

#title-start.active {
  opacity: 1;
}
